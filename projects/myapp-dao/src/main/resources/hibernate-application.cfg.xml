<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.dialect">${hibernate.application.dialect}</property>
        <mapping resource="hibernate/application/UserAttribute.hbm.xml" />
        <mapping resource="hibernate/application/SequentialNumberManage.hbm.xml" />
        <mapping resource="hibernate/application/PostalCode.hbm.xml" />
    </session-factory>
</hibernate-configuration>
