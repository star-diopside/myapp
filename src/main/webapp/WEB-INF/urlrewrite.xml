<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>

    <outbound-rule encodefirst="true">
        <from>(.*);jsessionid=[0-9A-Fa-f]{32}(.*)</from>
        <to>$1$2</to>
    </outbound-rule>

</urlrewrite>
