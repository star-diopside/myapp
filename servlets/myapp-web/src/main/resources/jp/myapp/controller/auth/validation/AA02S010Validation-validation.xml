<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
    "-//Apache Struts//XWork Validator 1.0.2//EN"
    "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>

    <field name="AA02S010Form.userId">
        <field-validator type="requiredstring">
            <message key="Error.NotInputUserId" />
        </field-validator>
    </field>

    <field name="AA02S010Form.userName">
        <field-validator type="requiredstring">
            <message key="Error.NotInputUserName" />
        </field-validator>
    </field>

    <field name="AA02S010Form.password">
        <field-validator type="requiredstring">
            <message key="Error.NotInputPassword" />
        </field-validator>
    </field>

</validators>
